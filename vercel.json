{
    "version": 2,
    "builds": [
        {
            "src": "src/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        },
        {
            "src": "**/*/.handlebars",
            "use": "handlebars",
            "config": {
                "extensions": ["handlebars"]
            }
        }
    ]
}
